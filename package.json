{
  "name": "icon-sprite",
  "version": "0.1.1",
  "type": "module",
  "description": "Combines svg icons into a single sprite file.",
  "author": "Brett Wood",
  "keywords": [
    "combine",
    "merge",
    "svg",
    "sprite",
    "icons"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/woodbrettm/icon-sprite"
  },
  "license": "MIT",
  "bugs": "https://github.com/woodbrettm/icon-sprite/issues",
  "homepage": "https://github.com/woodbrettm/icon-sprite",
  "scripts": {
    "build": "vite build",
    "test": "vitest --run",
    "test:ci": "vitest --run --coverage",
    "coverage": "vitest run --coverage",
    "demo-sprite": "ts-node ./scripts/demo-sprite.ts"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "dependencies": {
    "prettier": "3.0.3"
  },
  "devDependencies": {
    "@types/node": "20.8.5",
    "@vitest/coverage-v8": "0.34.6",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "vite-plugin-dts": "3.6.1",
    "vitest": "0.34.6"
  }
}