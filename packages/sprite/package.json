{
  "$schema": "https://json.schemastore.org/package",
  "name": "icon-sprite",
  "version": "0.1.3",
  "type": "module",
  "license": "MIT",
  "description": "Combines svg icons into a single sprite file.",
  "author": {
    "name": "Brett Wood",
    "url": "https://github.com/woodbrettm"
  },
  "bugs": "https://github.com/woodbrettm/icon-sprite/issues",
  "homepage": "https://github.com/woodbrettm/icon-sprite",
  "repository": {
    "type": "git",
    "url": "https://github.com/woodbrettm/icon-sprite",
    "directory": "packages/sprite"
  },
  "keywords": [
    "combine",
    "merge",
    "svg",
    "sprite",
    "icons"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=7.0.0"
  },
  "main": "./build/lib/main.js",
  "types": "./build/lib/main.d.ts",
  "module": "./build/lib/main.js",
  "exports": {
    ".": {
      "import": "./build/lib/index.js",
      "types": "./build/lib/index.d.ts"
    }
  },
  "files": [
    "build"
  ],
  "bin": {
    "iconsprite": "./build/bin/iconsprite.js"
  },
  "dependencies": {
    "c12": "1.5.1",
    "citty": "0.1.5",
    "consola": "3.2.3",
    "globby": "14.0.0",
    "ora": "7.0.1",
    "pathe": "1.1.1",
    "klona": "2.0.6",
    "deepmerge-ts": "5.1.0",
    "defu": "6.1.3"
  },
  "devDependencies": {
    "@types/node": "20.10.5",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "ts-essentials": "9.4.1",
    "@total-typescript/ts-reset": "0.5.1"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "check:tests": "vitest --run --coverage",
    "check:types": "tsc",
    "check:eslint": "eslint --quiet ./src/**/*.{ts,js}",
    "check:prettier": "prettier --check **/*.{md,json,yml,yaml}",
    "changeset:add": "changeset",
    "changeset:version": "changeset version",
    "changeset:release": "changeset release",
    "demo-sprite": "esrun ./demo/generate-sprite.ts"
  }
}
